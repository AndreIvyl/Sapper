        </menus>
      </MenuBar>
      <GridPane fx:id="gridBoard" gridLinesVisible="true" maxHeight="400.0" maxWidth="1.7976931348623157E308" minHeight="400.0" onMouseClicked="#gridClick" prefHeight="400.0">
        <columnConstraints>
